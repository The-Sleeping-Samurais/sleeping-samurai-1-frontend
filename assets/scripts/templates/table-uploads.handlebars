{{#each uploads as |upload|}}
<tr class="content">
    <td><p class="table-data">{{upload.name}}</p></td>
    <td><p class="table-data">{{upload.description}}</p></td>
    <td><p class="table-data">{{upload.tag}}</p></td>
    <td><a class="table-data" href="{{upload.url}}" target="_blank">Click Me</a></td>
    <td><p class="table-data">{{upload.createdAt}}</p></td>
    <td><p class="table-data">{{upload.updatedAt}}</p></td>
    <td>
    <button data-id="{{upload.id}}" class="remove-button btn btn-primary btn-sm" type="submit">delete</button>
    <button class="update-button btn btn-primary btn-sm" data-toggle="modal" data-target="#editModal" data-id="{{upload.id}}" data-name="{{upload.name}}" data-des="{{upload.description}}" data-tag="{{upload.tag}}" type="button">
       edit
       </button>
    </td>
  </tr>
{{/each}}
